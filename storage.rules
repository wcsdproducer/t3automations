rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: INSECURE - DO NOT USE IN PRODUCTION
    // This rule allows anyone on the internet to read, write, and delete
    // any file in your storage bucket. It is provided for temporary
    // debugging only.
    match /{allPaths=**} {
      allow read, write;
    }
  }
}
